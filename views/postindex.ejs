<!DOCTYPE html>
<html lang="en">
   
    <body>  
		

        <section>
            <div>
                <h2 class="title">Post Feed</h2>
            </div>
        </section>

        <main class="container">
            <div>
                <% for( let i = 0; i<posts.length; i++){ %>
                    <% const firstPost = posts[i] %>
                    <ul>
                    <li><a href="/post/<%= firstPost._id %>"><%= firstPost.post %></a> </li>

                    </ul>
                <% } %> 
            </div>
        </main>
    
	</body>
</html>